{
    "name": "poc-theme-switcher",
    "version": "1.0.0",
    "description": "UI5 Runtime Theme Switching POC with Splash Screen",
    "scripts": {
        "start": "npm run smart-start:cdn",
        "start:cdn": "node build.js cdn && fiori run --open --port ${PORT:-8300}",
        "start:local": "node build.js local && fiori run --open --port ${PORT:-8300}",
        "start:hybrid": "node build.js hybrid && fiori run --open --port ${PORT:-8300} --config ui5-backend.yaml",
        "start:backend": "node build.js backend && fiori run --open --port ${PORT:-8300}",
        "smart-start:cdn": "node start.js cdn",
        "smart-start:local": "node start.js local",
        "smart-start:backend": "node start.js backend",
        "smart-start:hybrid": "node start.js hybrid",
        "build": "node build.js",
        "build:ui5": "npx @ui5/cli build",
        "serve:cdn": "fiori run --port ${PORT:-8300} --open",
        "serve:local": "fiori run --port ${PORT:-8300} --open",
        "serve:backend": "fiori run --port ${PORT:-8300} --open",
        "serve:hybrid": "fiori run --port ${PORT:-8300} --open --config ui5-backend.yaml",
        "pdf": "md-to-pdf wms-integration/INTEGRATION-GUIDE.md && md-to-pdf wms-integration/INTEGRATION-GUIDE-TLDR.md && md-to-pdf wms-integration/USAGE-GUIDE.md && md-to-pdf wms-integration/USAGE-GUIDE-TLDR.md"
    },
    "keywords": [
        "ui5",
        "sapui5",
        "theme-switching",
        "splash-screen",
        "sap"
    ],
    "author": "ac4y",
    "license": "MIT",
    "devDependencies": {
        "@sap/ux-ui5-tooling": "^1",
        "@ui5/cli": "^4.0.43",
        "http-server": "^14.1.1",
        "md-to-pdf": "^5.2.5",
        "ui5-middleware-simpleproxy": "^3.6.2"
    }
}
