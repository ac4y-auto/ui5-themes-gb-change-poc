{
    "name": "poc-theme-switcher",
    "version": "1.0.0",
    "description": "UI5 Runtime Theme Switching POC with Splash Screen",
    "scripts": {
        "start": "npm run smart-start:cdn",
        "start:cdn": "node build.js cdn && npx http-server webapp -p ${PORT:-8300} --cors -o",
        "start:local": "node build.js local && npx @ui5/cli serve --port ${PORT:-8300} --open",
        "start:hybrid": "node build.js hybrid && npx @ui5/cli serve --port ${PORT:-8300} --config ui5-backend.yaml --open",
        "start:backend": "node build.js backend && npx http-server webapp -p ${PORT:-8300} --cors -o",
        "smart-start:cdn": "node start.js cdn",
        "smart-start:local": "node start.js local",
        "smart-start:backend": "node start.js backend",
        "smart-start:hybrid": "node start.js hybrid",
        "build": "node build.js",
        "build:ui5": "npx @ui5/cli build",
        "serve:cdn": "npx http-server webapp -p ${PORT:-8300} --cors -o",
        "serve:local": "npx @ui5/cli serve --port ${PORT:-8300} --open",
        "serve:backend": "npx http-server webapp -p ${PORT:-8300} --cors -o",
        "serve:hybrid": "npx @ui5/cli serve --port ${PORT:-8300} --config ui5-backend.yaml --open"
    },
    "keywords": [
        "ui5",
        "sapui5",
        "theme-switching",
        "splash-screen",
        "sap"
    ],
    "author": "ac4y",
    "license": "MIT",
    "devDependencies": {
        "@ui5/cli": "^4.0.43",
        "http-server": "^14.1.1",
        "ui5-middleware-simpleproxy": "^3.6.2"
    }
}
